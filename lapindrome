/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
    public static boolean areEqual(String s1 , String s2)
    {
        int CHAR = 26;
        int count1[] = new int[CHAR];
        int count2[] = new int[CHAR];
        int str_len =s1.length();
        for(int i=0;i<str_len;++i)
        {
           count1[s1.charAt(i)-'a']++;
           count2[s2.charAt(i)-'a']++;
        }
        for (int i = 0; i < CHAR; i++) 
            if (count1[i] != count2[i]) 
                return false; 
       
        return true; 
    }
    public static boolean checkLapindromes(String s)
    {
        int len = s.length();
        int midIndex=0;
        if(len%2==0)
              midIndex =len/2;
        else 
              midIndex = (len/2)+1;
        String first = s.substring(0,midIndex);
        String second = s.substring(midIndex,len);
        boolean res = areEqual(first,second);
        return res;
             
    }
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
	   BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
	   int testcases = Integer.parseInt(br.readLine());
		while(testcases-- > 0)
		{
		    String text = br.readLine() ;
		    boolean res = checkLapindromes(text);
		    if(res) 
		       System.out.println("YES");
		    else 
		       System.out.println("NO");
		}
		
	}
}
